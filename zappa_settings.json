{
    "dev": {
        "app_function": "run.app",
        "aws_region": "ap-northeast-1",
        "profile_name": "epa-zappa",
        "project_name": "epa",
        "runtime": "python3.8",
        "s3_bucket": "epa-bucket",
        "role_name":"epa-dev-ZappaLambdaExecutionRole",
        "exclude": [
            "__pycache__",
            "_cache",
            ".ipynb_checkpoints",
            ".vscode",
            "node_modules",
            "prod_venv",
            "venv",
            ".git",
            ".gitignore",
            "config.env",
            "config-*",
            "Dockerfile",
            "README.md",
            "flake8*",
            "poetry*",
            "boto3", 
            "boto3-*", 
            "botocore*",
            ".python-version"
        ]
